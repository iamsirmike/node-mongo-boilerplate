{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist"
  },
  "lib": ["es2015"],
  "include": ["src", "app.ts", "server.ts", "test"],
  "exclude": ["node_modules"],
}